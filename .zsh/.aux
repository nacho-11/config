### Aux Functions ###

function rZsh { 
	source ~/.zshrc
}

function cdp {
	cd $MAIN_FOLDER/Projects
}

function cdu {
	cd $MAIN_FOLDER/University
}

function c {
	clear
}

function folder {
	nautilus . 2&>/dev/null &
	disown
}

function config {
	cd $CONFIG
	code .
}

#@rZsh      reset zsh
#@cdp       move to projects folder
#@cdu       move to university folder
#@c         clear the console
#@folder    open nautilus in current directory
#@config    open and move config folder
